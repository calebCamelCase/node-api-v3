<%- include('../partials/header'); %>

<main class="main main-customer container">
    <section class="section section-customer">
        <div class="row">
            <% data.forEach(item => { %>
                <div class="col-md-3">
                    <figure class="figure item-div">
                        <img src="https://via.placeholder.com/200x250" alt="placeholder" class="img-fluid placeholder-image" />
                        <figcaption class="item-h2">
                            <a href="/customer/<%= item.customer_id %>"><%= item.first_name %> <%= item.last_name %></a>

                        </figcaption>
                    </figure>
                </div>
            <% }) %>
        </div>
    </section>
</main>

<%- include('../partials/footer'); %>